<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:cultures="clr-namespace:WPFLocalize.Cultures"
	x:Class="WPFLocalize.UserControl2"
	x:Name="UserControl"
	Width="100" Height="100">
  
  <Grid x:Name="LayoutRoot" Background="#FF9B2929">
  	<Label HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="labelUserControl2" Content="{Binding Path=LabelCultureName, Source={StaticResource Resources}}"/>
	</Grid>
</UserControl>